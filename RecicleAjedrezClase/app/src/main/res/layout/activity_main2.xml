<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pantalla2">

    <Button
        android:id="@+id/b_volver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.887" />

    <TableLayout
        android:layout_width="381dp"
        android:layout_height="504dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/i_pantalla2"
                android:layout_width="137dp"
                android:layout_height="96dp" />

            <TextView
                android:id="@+id/tv_Nombre"
                android:layout_width="113dp"
                android:layout_height="51dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/tv_nacionalidad"
                android:layout_width="129dp"
                android:layout_height="51dp"
                android:text="Noruega"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/asdfdfssaf"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:text="Mundiales:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_campeonatos"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart" />

        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/asdfsdfdfssaf"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:text="Porcentaje Victorias:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_porcentaje"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/fdfe"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:text="Rankin FIFE:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_ranking"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/asdff"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:text="Estilo:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_estilo"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/fsdf"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:text="Mayor Rivalidad:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_rivalidad"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="147dp"
                android:layout_height="match_parent"
                android:text="Curiosidad: "
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_curiosidad"
                android:layout_width="233dp"
                android:layout_height="50dp"
                android:text="TextView"
                android:textAlignment="viewStart" />

        </TableRow>

    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>